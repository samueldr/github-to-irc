{
	"github-to-irc": {
		"channels": {
			"default": [
				"#nixos"
			],
			"per-repository": {
				"nixos/ofborg": [
					"#nixos-borg"
				]
			}
		},
		"filters" : {
			"default": [
				{"type": "ignore-branch", "exact": "gh-pages"}
			],
			"per-repository": {
				"nixos/nixpkgs": [
					{"type": "ignore-branch", "regex": "^nix(os|pkgs)-"}
				]
			}
		}
	},
	"rabbitmq": {
		"vhost": "irc",
		"username": "webhook",
		"password": "webhook",
		"host": "127.0.0.1",
		"ssl": false
	}
}
